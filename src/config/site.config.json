  "serverrules": [
    {
      "srid": "rdm",
      "srtitle": "No Random Deathmatch (RDM)",
      "srdescription": "Killing another player without a valid in-character reason is strictly prohibited.",
      "srroleplayContext": "RDM disrupts immersive roleplay and can ruin other players’ experience. Only engage in combat when it is justified by the storyline.",
      "srenforcement": "",
      "srexamples": [],
      "srcommonMistakes": [],
      "srnotes": "",
      "sricon": "Shield"
    },
    {
      "srid": "vdm",
      "srtitle": "No Vehicle Deathmatch (VDM)",
      "srdescription": "Using vehicles to intentionally harm or kill other players without proper RP context is forbidden.",
      "srroleplayContext": "VDM breaks immersion and is unfair. Vehicles should be tools for roleplay, not instant kills.",
      "srenforcement": "Immediate warning for minor incidents. Repeated or intentional VDM: temporary or permanent bans depending on severity.",
      "srexamples": [
        "Running over a player in a car outside a roleplay scenario.",
        "Using a vehicle to chase and kill another player without RP justification."
      ],
      "srcommonMistakes": [
        "Driving recklessly during car chases that are RP-approved.",
        "Confusing accidents with intentional VDM."
      ],
      "srnotes": "Make sure any vehicle use for combat is clearly RP-driven and agreed upon by participants.",
      "sricon": "Users"
    },
    {
      "srid": "metagaming",
      "srtitle": "No Metagaming",
      "srdescription": "Using out-of-character information in-character is strictly forbidden.",
      "srroleplayContext": "Metagaming gives unfair advantages and breaks immersion. Always separate IC (in-character) knowledge from OOC (out-of-character) knowledge.",
      "srenforcement": "First offense: warning. Repeated offenses: temporary ban (1-7 days). Severe or repeated cases: permanent ban.",
      "srexamples": [
        "Using someone’s Discord messages to make IC decisions.",
        "Knowing forum or ticket information to influence in-game actions."
      ],
      "srcommonMistakes": [
        "Accidentally using IC knowledge learned out-of-character without intent.",
        "Sharing OOC info to benefit your character."
      ],
      "srnotes": "If unsure whether info is OOC or IC, do not use it.",
      "icon": "Eye"
    },
    {
      "srid": "powergaming",
      "srtitle": "No Powergaming",
      "srdescription": "Forcing actions on others without giving them a chance to react is prohibited.",
      "srroleplayContext": "Powergaming removes player agency and breaks the flow of RP. Every action should allow a response from affected players.",
      "srenforcement": "Warnings for minor offenses; temporary ban for repeated violations; permanent ban for severe cases.",
      "srexamples": [
        "Dragging someone into a car without letting them react.",
        "Instantly performing actions like healing or attacking without the other player being able to respond."
      ],
      "srommonMistakes": [
        "Assuming automatic success in contested actions.",
        "Not giving others a chance to roleplay their reactions."
      ],
      "srnotes": "All roleplay interactions must allow for natural reactions and responses.",
      "sricon": "Lock"
    },
    {
      "srid": "respect",
      "srtitle": "Respect Everyone",
      "srdescription": "All players and staff must be treated with respect. Harassment, bullying, or discrimination is prohibited.",
      "srroleplayContext": "A respectful environment ensures an enjoyable experience for all players. Toxic behavior will not be tolerated.",
      "srenforcement": "Disrespectful behavior may result in warnings, chat/mute penalties, temporary bans, or permanent bans depending on severity.",
      "srexamples": [
        "Using offensive language towards other players or staff.",
        "Intentionally sabotaging events or interactions for harassment purposes."
      ],
      "srcommonMistakes": [
        "Ignoring staff instructions during events.",
        "Unintentional insults or teasing that disrupts community harmony."
      ],
      "srnotes": "Maintain a positive environment. Always resolve conflicts calmly and respectfully.",
      "sricon": "Database"
    }
  ],
